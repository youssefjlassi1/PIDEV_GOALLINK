            <BorderPane>
                <top>
                   
                </top>
            </BorderPane>
            <VBox spacing="10.0" HBox.hgrow="ALWAYS">
                <HBox.margin>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </HBox.margin>
                <Text fx:id="nameText" text="Name" VBox.vgrow="ALWAYS">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Text>
               
                <Text fx:id="statusText" text="Status" wrappingWidth="190" VBox.vgrow="ALWAYS">
                    <font>
                        <Font name="System Bold" size="15.0"/>
