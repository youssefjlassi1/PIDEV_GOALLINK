                </Text>
               
               
               
               
                <Text fx:id="descriptionText" text="Description" wrappingWidth="400.0" VBox.vgrow="ALWAYS">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Text>

            </VBox>

        

        </HBox>
    </AnchorPane>
