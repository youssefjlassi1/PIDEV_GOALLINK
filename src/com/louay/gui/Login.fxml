<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="500.0" style="-fx-background-color: white"
            stylesheets="@../utils/MainStyle.css" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.louay.gui.LoginController">

    <Button layoutX="100.0" layoutY="38.0" mnemonicParsing="false" onAction="#frontend" text="Front" textFill="#FFFFFF"
            AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="60.0">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Button>


    <VBox alignment="CENTER" spacing="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          HBox.hgrow="ALWAYS">
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
        </padding>
        <ComboBox fx:id="userCB" promptText="Choisir user" styleClass="form-control"/>

    </VBox>


    <Button mnemonicParsing="false" onAction="#backend" text="Back" textFill="#FFFFFF" AnchorPane.bottomAnchor="50.0"
            AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Button>
</AnchorPane>
