    <children>
        <VBox prefHeight="300.0" prefWidth="400.0">
            <TableView fx:id="leaderboardTable" prefHeight="300.0" prefWidth="400.0">
         <columns>
             <TableColumn fx:id="nameColumn" prefWidth="250.0" text="Name" />
             <TableColumn fx:id="rankColumn" prefWidth="75.0" text="Rank" />
             <TableColumn fx:id="lpColumn" prefWidth="75.0" text="LP" />
         </columns>
            </TableView>
        </VBox>
    </children>
   </AnchorPane>




