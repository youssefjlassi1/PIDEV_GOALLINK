<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import java.lang.String?>
<AnchorPane id="AnchorPane" maxHeight="1.7976931348623157E308" minHeight="-Infinity" minWidth="800.0" prefHeight="500.0"
            style="-fx-background-color: #EEEEEE;" stylesheets="@../../../utils/MainStyle.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.louay.gui.back.Request.ShowAllController">
    <AnchorPane style="-fx-background-color: #888888;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0">
        <StackPane prefHeight="70.0" AnchorPane.leftAnchor="30.0" AnchorPane.topAnchor="0.0">
            <Text fx:id="topText" boundsType="LOGICAL_VERTICAL_CENTER" fill="WHITE" strokeType="OUTSIDE"
                  strokeWidth="0.0" text="Feedbacks" textAlignment="JUSTIFY" textOrigin="CENTER">
                <font>
                    <Font name="Arial Bold" size="23.0"/>
                </font>
            </Text>
        </StackPane>
        <TextField fx:id="searchTF" alignment="CENTER" onKeyReleased="#search" prefHeight="50.0" prefWidth="350.0"
                   promptText="Rechercher par Sujet" styleClass="form-control-white" AnchorPane.leftAnchor="250.0"
                   AnchorPane.topAnchor="10.0">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
        </TextField>

        <Button fx:id="exelButton" layoutX="670.0" layoutY="10.0" mnemonicParsing="false" onAction="#makeExel"
                prefHeight="51.0" prefWidth="116.0" style="-fx-background-color: green;" text="Generer Excel"
                textAlignment="CENTER" textFill="WHITE" wrapText="true" AnchorPane.rightAnchor="14.0"
                AnchorPane.topAnchor="10.0">
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
            <styleClass>
                <String fx:value="window-rounded"/>
                <String fx:value="border-gray"/>
                <String fx:value="border-rounded"/>
            </styleClass>
        </Button>
    </AnchorPane>
    <ScrollPane fitToHeight="true" fitToWidth="true" layoutY="140.0" maxHeight="-Infinity" maxWidth="-Infinity"
                pannable="true" style="-fx-background-color: transparent;" AnchorPane.bottomAnchor="0.0"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="70.0">
        <opaqueInsets>
            <Insets/>
        </opaqueInsets>
        <AnchorPane>
            <VBox fx:id="mainVBox" layoutY="-2.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                  AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        </AnchorPane>
    </ScrollPane>
</AnchorPane>
