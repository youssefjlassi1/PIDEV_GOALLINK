<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="500.0" stylesheets="@../utils/MainStyle.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.pi.gui.LoginController" style="-fx-background-color: white">

    <Button layoutX="100.0" layoutY="38.0" mnemonicParsing="false" styleClass="success-button" onAction="#frontend"
            text="Front" textFill="#FFFFFF" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Button>

    <Button mnemonicParsing="false" onAction="#backend" styleClass="success-button" text="Back" textFill="#FFFFFF"
            AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0">
        <font>
            <Font name="System Bold" size="16.0"/>
        </font>
    </Button>
</AnchorPane>
